pipeline {
  agent any 
  tools {
    maven 'maven'
    }
  stages {
    stage('Checkout') {
    environment {
        GIT_CREDENTIALS = credentials('GIT_CREDENTIALS')
    }
      steps {
        sh 'echo passed'
        
        git credentials: GIT_CREDENTIALS, git branch: 'main', url: 'https://github.com/deleonab/java-maven-sonar-argocd-helm-kubernetes.git'
      }
    }
    
  }
}
