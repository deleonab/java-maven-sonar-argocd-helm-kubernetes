pipeline {
  agent any 
  
  stages {
    stage('Checkout') {
    environment {
        GIT_CREDENTIALS = credentials('GIT_CREDENTIALS')
    }
      steps {
        sh 'echo passed'
        
        git branch: 'main', credentialsId: GIT_CREDENTIALS, url: 'https://github.com/deleonab/java-maven-sonar-argocd-helm-kubernetes.git'
      }
    }
    
  }
}
